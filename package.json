{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "boilerplate",
  "main": "index.js",
  "scripts": {
    "migrate": "node ./migrations/migrateUp.js",
    "migrate:undo": "node ./migrations/migrateDown.js",
    "seed": "node ./seeders/seed.js",
    "test": "jest --verbose",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "start": "nodemon --exec 'env DEBUG=provider:* node' ./src/index.js"
  },
  "author": "Mehmood Hussain",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "joi": "^17.11.0",
    "joi-password-complexity": "^5.2.0",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.4.4",
    "nodemailer": "^6.9.8",
    "path": "^0.12.7",
    "pg": "^8.12.0",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.1"
  }
}
